<!DOCTYPE html>
<html ng-app='myApp'>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title ng-bind="title"></title>

    <!-- Bootstrap -->
    <link href="/static/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/static/css/app.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <% if(serialized){ %>
    <script>
        var startUpUserData = '<%- serialized %>';
    </script>
    <% }else{ %>
    <% } %>
</head>

<body ng-cloak>
<div ng-controller="HeaderController">
    <div class="navbar navbar-default navbar-static-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" ng-click="navCollapsed = !navCollapsed">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Learn</a>
            </div>
            <div class="collapse navbar-collapse" uib-collapse="navCollapsed" ng-class="!navCollapsed && 'in'">
                <ul class="nav navbar-nav">
                    <li ng-repeat="link in headerLinks" ng-class="{active:link.active}">
                        <a ng-click="goToState(link.state)">{{link.title}}</a>
                    </li>
                </ul>

                <div uib-dropdown ng-show="authenticated" class="nav navbar-nav pull-right hidden-xs">
                    <a href="#" class="btn navbar-btn btn-default" uib-dropdown-toggle role="button">
                        <span class="glyphicon glyphicon-cog"></span>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li role="menuitem">
                            <a ui-sref="dashboard.profile" ng-click="navCollapsed = true">
                                <span class="glyphicon glyphicon-dashboard"></span> ჩემი პანელი</a>
                        </li>
                        <!--<li class="divider"></li>-->
                        <li role="menuitem">
                            <a ng-click="logOut(); navCollapsed = true"><span class="glyphicon glyphicon-off"></span>
                                გამოსვლა</a>
                        </li>
                    </ul>
                </div>
                <div ng-show="authenticated" class="nav navbar-nav navbar-right visible-xs">
                    <a ui-sref="dashboard.profile" ng-click="navCollapsed = !navCollapsed">
                    <span class="glyphicon glyphicon-dashboard"></span> ჩემი პანელი</a>
                    <a ng-click="logOut(); navCollapsed = true"><span class="glyphicon glyphicon-off"></span>
                            გამოსვლა</a>
                </div>

                <ul ng-show="!authenticated" class="nav navbar-nav navbar-right">
                    <li><a ui-sref="login" ng-click="navCollapsed = true">შესვლა</a></li>
                    <li><a ui-sref="register" ng-click="navCollapsed = true">რეგისტრაცია</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </div>
</div>

<div ui-view class="container"></div>

<div class="footer">
    <div class="container">
        <div class="row">
            <div class="col-xs-6">
                <div class="footer-text">&copy; 2016, By G!KH</div>
            </div>
            <div class="col-xs-6">
                <div class="footer-text pull-right"><a ui-sref="contact">კონტაქტი</a></div>
            </div>
        </div>
    </div>
</div>

